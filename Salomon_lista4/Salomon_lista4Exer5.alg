algoritmo "semnome"
// Função :
// Autor :
// Data : 21/03/2017
// Seção de Declarações 
var
joao:real
ze:real
cont:inteiro
inicio
      joao<-1.50
      ze<-1.10
      enquanto (joao>ze) faca
      joao<- joao+0.02
      ze<- ze+0.03
      cont<- cont+1
      fimenquanto
      escreval("sao necessarios:",cont," anos para que ze seja maior")

fimalgoritmo
