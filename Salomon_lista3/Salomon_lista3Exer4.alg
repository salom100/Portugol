algoritmo "semnome"
// Função :
// Autor :
// Data : 14/03/2017
// Seção de Declarações 
var
v1:real
v2:real
inicio
 leia(v1)
 leia(v2)
 se v1>v2 entao
 escreval("o valor 1 e maior que o segundo")
 senao
 se v1<v2 entao
 escreval("o valor 2 e maior que o primeiro")
 senao
 escreval("os dois valores sao iguais")
 fimse
 fimse

fimalgoritmo
