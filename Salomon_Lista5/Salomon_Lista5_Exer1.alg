algoritmo "semnome"
// Função :
// Autor :
// Data : 27/03/2017
// Seção de Declarações 
var
cont:inteiro
soma:inteiro
inicio
    cont<- 1
    repita
          Escreval(cont)
          soma<-soma+cont
          cont<- cont+1

    ate (cont>100)

    Escreval("a soma dos 100 primeiros numeros é:",soma)

fimalgoritmo
